{
    "manifest_version": 2,

    "name": "Wikipedia Visual History (alpha)",
    "version": "0.0.1",

    "description": "Visualization of your history in wikipedia.",

    "permissions": [
    "history",
    "storage",
    "tabs",
    "http://ja.wikipedia.org/"
    ],

    "background": {
        "scripts":["background.js"]
    },

    "browser_action": {
        "default_icon": "icon.png"
    }
}
